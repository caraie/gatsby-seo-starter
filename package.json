{
    "name": "gatsby-starter-default",
    "description": "Gatsby default starter",
    "version": "1.0.0",
    "author": "Kyle Mathews <mathews.kyle@gmail.com>",
    "dependencies": {
        "gatsby": "^1.9.241",
        "gatsby-link": "^1.6.39",
        "gatsby-plugin-react-helmet": "^2.0.8",
        "react-helmet": "^5.2.0"
    },
    "keywords": [
        "gatsby"
    ],
    "license": "MIT",
    "scripts": {
        "clean": "rm -rf public/* && rm -rf .cache/*",
        "build": "npm run clean && node_modules/.bin/gatsby build",
        "deploy": "npm run clean && npm run build:pp && node_modules/.bin/gh-pages -d public -b master",
        "build:pp": "npm run clean && node_modules/.bin/gatsby build --prefix-paths",
        "serve": "node_modules/.bin/gatsby serve",
        "develop": "npm run clean && node_modules/.bin/gatsby develop",
        "lint": "npm run lint:ts && npm run lint:md && npm run lint:js",
        "lint:ts": "node_modules/.bin/tslint \"./src/**/*.ts\" \"./src/**/*.tsx\"",
        "lint:md": "node_modules/remark/remark.js content/posts/",
        "lint:js": "node_modules/.bin/eslint --ext .js,.jsx .",
        "stylelint": "node_modules/.bin/stylelint \"./src/**/*.css\"",
        "write-good": "node_modules/.bin/write-good $(glob 'content/posts/**/*.md')",
        "beautify": "npm run beautify:js && npm run beautify:css",
        "beautify:js": "node_modules/.bin/js-beautify --replace --type js -f ./*.js",
        "beautify:css": "node_modules/.bin/js-beautify --replace --type css -f ./src/**/*.css",
        "format": "node_modules/.bin/prettier --parser typescript --single-quote=false --trailing-comma es5 --no-semi --write \"src/**/*.ts*\"",
        "format:js": "node_modules/.bin/prettier --single-quote=false --trailing-comma es5 --no-semi --write \"./*.{js, jsx}\""
    },
    "devDependencies": {
        "@types/algoliasearch": "^3.24.9",
        "@types/node": "^9.6.0",
        "@types/react": "^16.0.41",
        "@types/react-dom": "^16.0.4",
        "@types/react-helmet": "^5.0.5",
        "algoliasearch": "^3.26.0",
        "bluebird": "^3.5.1",
        "eslint": "^4.19.1",
        "eslint-config-airbnb": "^16.1.0",
        "eslint-config-prettier": "^2.9.0",
        "eslint-plugin-import": "^2.9.0",
        "eslint-plugin-jsx-a11y": "^6.0.3",
        "gatsby": "^1.9.241",
        "gatsby-image": "^1.0.42",
        "gatsby-link": "^1.6.39",
        "gatsby-plugin-algolia": "0.0.3",
        "gatsby-plugin-catch-links": "^1.0.19",
        "gatsby-plugin-debug-build": "^1.0.0",
        "gatsby-plugin-facebook-analytics": "^1.0.3",
        "gatsby-plugin-fastclick": "^1.0.1",
        "gatsby-plugin-google-analytics": "^1.0.24",
        "gatsby-plugin-manifest": "^1.0.15",
        "gatsby-plugin-nprogress": "^1.0.14",
        "gatsby-plugin-offline": "^1.0.15",
        "gatsby-plugin-react-helmet": "^2.0.8",
        "gatsby-plugin-sharp": "^1.6.41",
        "gatsby-plugin-sitemap": "^1.2.20",
        "gatsby-plugin-styled-components": "^2.0.11",
        "gatsby-plugin-twitter": "^1.0.20",
        "gatsby-plugin-typescript": "^1.4.19",
        "gatsby-plugin-webpack-bundle-analyzer": "^0.1.1",
        "gatsby-remark-autolink-headers": "^1.4.13",
        "gatsby-remark-copy-linked-files": "^1.5.30",
        "gatsby-remark-images": "^1.5.59",
        "gatsby-remark-prismjs": "^1.2.21",
        "gatsby-remark-responsive-iframe": "^1.4.18",
        "gatsby-remark-responsive-image": "^1.0.0-beta.5-alpha.fb30fcd6",
        "gatsby-remark-smartypants": "^1.4.12",
        "gatsby-source-filesystem": "^1.5.27",
        "gatsby-source-github-api": "0.0.3",
        "gatsby-transformer-remark": "^1.7.37",
        "gatsby-transformer-sharp": "^1.6.22",
        "gh-pages": "^1.1.0",
        "graphql-tag": "^2.8.0",
        "js-beautify": "^1.7.5",
        "lodash": "^4.17.5",
        "material-ui": "^1.0.0-beta.39",
        "material-ui-icons": "^1.0.0-beta.36",
        "prettier": "^1.11.1",
        "react-disqus-thread": "^0.4.0",
        "react-helmet": "^5.2.0",
        "react-icons": "^2.2.7",
        "react-instantsearch": "^5.0.1",
        "react-markdown": "^3.3.0",
        "react-render-html": "^0.6.0",
        "react-share": "^2.0.0",
        "remark": "^9.0.0",
        "remark-cli": "^5.0.0",
        "remark-preset-lint-recommended": "^3.0.1",
        "styled-components": "^3.2.3",
        "stylelint": "^9.1.3",
        "stylelint-config-standard": "^18.2.0",
        "tslint": "^5.9.1",
        "tslint-microsoft-contrib": "^5.0.3",
        "typescript": "^2.7.2",
        "write-good": "^0.11.3"
    }
}
